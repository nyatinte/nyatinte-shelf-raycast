{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "nyatinte-shelf-raycast",
  "title": "nyatinte-shelf-raycast",
  "description": "private Raycast extension for nyatinte-shelf",
  "icon": "command-icon.png",
  "author": "nyatinte",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Add Document",
      "description": "Add document from inputted URL.",
      "mode": "no-view",
      "arguments": [
        {
          "name": "url",
          "type": "text",
          "required": true,
          "placeholder": "URL"
        }
      ]
    },
    {
      "name": "set-api-key",
      "title": "Set API Key",
      "description": "Set API Key for nyatinte-shelf.",
      "mode": "no-view",
      "arguments": [
        {
          "name": "apiKey",
          "type": "password",
          "required": true,
          "placeholder": "API Key"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  },
  "volta": {
    "node": "20.11.1",
    "npm": "10.5.0"
  }
}
